{
  "name": "my-tools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git-cz",
    "analyze": "ANALYZE=true yarn build",
    "svgr": "svgr --icon --replace-attr-values '#F2F2F2'={props.color} -d src src/icons/assets --out-dir src/icons/output --index-template icons-template.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@next/bundle-analyzer": "^12.2.0",
    "@uimakers/tools-ui": "^1.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "framer-motion": "^6.3.16",
    "next": "^12.2.0",
    "pdf-parser": "^1.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-lottie-player": "^1.5.0",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "prettier": "2.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browser": {
    "fs": false,
    "child_process": false,
    "os": false
  }
}
